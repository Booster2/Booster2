system RareDiseasesHeuristics
 


	class GELDiseaseGroup  { 		attributes			name (ID) : STRING			identifier  : STRING			subGroups  : set ( GELDiseaseSubGroup. parentGroups )  [0 .. *]		methods 			create { ((new gdg! : GELDiseaseGroup) || (gdg!.name := name?)) || (gdg!.identifier := identifier?) }			newSubGroup { (((new gdsg! : GELDiseaseSubGroup) || (gdsg!.name := name?)) || (gdsg!.identifier := identifier?)) || ((gdsg!.parentGroups := gdsg!.parentGroups \/ {this}) || (this.subGroups := this.subGroups \/ {gdsg!})) }	}	class GELDiseaseSubGroup  { 		attributes			name (ID) : STRING			identifier  : STRING			parentGroups  : set ( GELDiseaseGroup. subGroups )  [0 .. *]			specificDisorders  : set ( GELSpecificDisorder. subGroups )  [0 .. *]		methods 			create { ((new gdsg! : GELDiseaseSubGroup) || (gdsg!.name := name?)) || (gdsg!.identifier := identifier?) }			newSpecificDisorder { (((new gsd! : GELSpecificDisorder) || (gsd!.name := name?)) || (gsd!.identifier := identifier?)) || ((gsd!.subGroups := gsd!.subGroups \/ {this}) || (this.specificDisorders := this.specificDisorders \/ {gsd!})) }	}	class GELSpecificDisorder  { 		attributes			name (ID) : STRING			identifier  : STRING			subGroups  : set ( GELDiseaseSubGroup. specificDisorders )  [0 .. *]		methods 			create { ((new gsd! : GELSpecificDisorder) || (gsd!.name := name?)) || (gsd!.identifier := identifier?) }	}	class HPOPhenotype  { 		attributes			name (ID) : STRING			identifier (ID) : STRING			description  : STRING			broaderTerms  : set ( HPOPhenotype. narrowerTerms )  [0 .. *]			narrowerTerms  : set ( HPOPhenotype. broaderTerms )  [0 .. *]		methods 			create { (((new phen! : HPOPhenotype) || (phen!.name := name?)) || (phen!.identifier := identifier?)) || (phen!.description := description?) }			associateNarrowerTerm { (this.narrowerTerms := this.narrowerTerms \/ {narrowerTerm?}) || (narrowerTerm?.broaderTerms := narrowerTerm?.broaderTerms \/ {this}) }			associateBroaderTerm { (this.broaderTerms := this.broaderTerms \/ {broaderTerm?}) || (broaderTerm?.narrowerTerms := broaderTerm?.narrowerTerms \/ {this}) }	}	class Site  { 		attributes			name (ID) : STRING			identifier  : STRING	}