system ClassicCarsWp
 


	class Customer  { 		attributes			customerNumber (ID) : INT			customerName (ID) : STRING			contactLastName  : STRING			contactFirstName  : STRING			phone  : STRING			addressLine1  : STRING			addressLine2  : STRING			city  : STRING			state  : STRING			postalCode  : STRING			country  : STRING			salesRepEmployee  : Employee . salesRepFor			creditLimit  : STRING			orders  : set ( PurchaseOrder. customer )  [0 .. *]			payments  : set ( Payment. customer )  [0 .. *]	}	class Employee  { 		attributes			employeeNumber (ID) : INT			lastName (ID) : STRING			firstName (ID) : STRING			extension  : STRING			email  : STRING			office  : Office . employees			reportsTo  : [ Employee . inChargeOf ]			jobTitle  : STRING			inChargeOf  : set ( Employee. reportsTo )  [0 .. *]			salesRepFor  : set ( Customer. salesRepEmployee )  [0 .. *]			dateOfBirth  : DATETIME		methods 			update { (((dateOfBirth? : DATETIME & employeeNumber? : INT) & firstName? : STRING) & lastName? : STRING) --> ( (((this.dateOfBirth := dateOfBirth?) || (this.employeeNumber := employeeNumber?)) || (this.firstName := firstName?)) || (this.lastName := lastName?) ) }	}	class Office  { 		attributes			officeCode (ID) : INT			city (ID) : STRING			phone  : STRING			addressLine1  : STRING			addressLine2  : STRING			state  : STRING			country  : STRING			postalCode  : STRING			territory  : STRING			employees  : set ( Employee. office )  [0 .. *]	}	class Payment  { 		attributes			customer (ID) : Customer . payments			checkNumber (ID) : STRING			paymentDate  : STRING			amount  : STRING	}	class Product  { 		attributes			productName (ID) : STRING			productCode (ID) : STRING			productLine  : ProductLine . products			productScale  : STRING			productVendor  : STRING			productDescription  : STRING			quantityInStock  : INT			buyPrice  : STRING			MSRP  : STRING			forPurchaseOrders  : set ( PurchaseOrderDetails. product )  [0 .. *]	}	class ProductLine  { 		attributes			productLine (ID) : STRING			textDescription  : STRING			htmlDescription  : STRING			image  : STRING			products  : set ( Product. productLine )  [0 .. *]		methods 			create { true --> ( new p! : ProductLine ) }			create2 { true --> ( new pl! : ProductLine ) }			updateTextDescription { (("Classic Cars" /= this.productLine & "Hello, World!" /= textDescription?) & textDescription? : STRING) --> ( this.textDescription := textDescription? ) }	}	class PurchaseOrder  { 		attributes			orderNumber (ID) : INT			orderDate (ID) : STRING			requiredDate  : STRING			shippedDate  : STRING			status  : STRING			comments  : STRING			details  : set ( PurchaseOrderDetails. purchaseOrder )  [0 .. *]			customer  : Customer . orders	}	class PurchaseOrderDetails  { 		attributes			purchaseOrder  : PurchaseOrder . details			product (ID) : Product . forPurchaseOrders			quantityOrdered (ID) : INT			priceEach  : STRING			orderLineNumber  : INT	}