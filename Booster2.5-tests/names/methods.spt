module names-methods

language Booster25

setup Common [[
	system debug
]]

test Duplicate method fails [[
	class A {
		methods
			[[create]] { a! : A' }
			[[create]] { a! : A' }
	}
]] 2 errors

test Resolve to method from call  [[
	class A {
		attributes
			a : INT
		methods
			[[updateA]] { a' = a + 1 }
			m1 { this . [[updateA]] () & a > 3}		
	}
]] resolve #2 to #1
