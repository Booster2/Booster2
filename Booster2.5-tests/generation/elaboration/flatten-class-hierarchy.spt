module generation-elaboration-flatten-class-hierarchy

language Booster25

setup Common [[
	system debug
]]

test Flatten class hierarchy single attribute [[
	class A{
		attributes
			name : string
	}
	class B extends A{}
]] build elaborate-builder-ast to [[
	class A{
		attributes
			name : string
	}
	class B extends A{
		attributes
			name : string
	}
]] 

test Flatten class hierarchy single attribute transitively [[
	class A{
		attributes
			name : string
	}
	class B extends A{}
	class C extends B{}
]] build elaborate-builder-ast to [[
	class A{
		attributes
			name : string
	}
	class B extends A{
		attributes
			name : string
	}
	class C extends B{
		attributes
			name : string
	}
]] 