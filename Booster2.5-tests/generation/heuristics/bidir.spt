module generation-heuristics-bidir

language Booster25

setup Common [[
system debug
	
	set DayOfWeek { Monday, Tuesday, Wednesday}
	
	class A {
		attributes
			b : B . a
			bs : SET ( B.as )
		methods
]]

test Heuristics Bidir mand-mand [[
	m1 { b' = b! }
]] build heuristics-builder-ast to [[
	m1 { b' = b! | b! /: B --> ( new b ! : B ; b := b! || b!.a := this ) }
]]

test Heuristics Bidir set-set [[
	m2 { b? : bs' }
]] build heuristics-builder-ast to [[
	m2 { b? : bs' | bs := bs \/ { b? } || b?.as := b?.as \/ { this } }
]]

setup Teardown [[
	}
	
class B {
	attributes
		a : A . b
		as : SET ( A.bs )
}
]]
