conjoin:
	Parallel(p,q) -> Parallels(<union>([p],[q]))		

conjoin:
	And(Conjuncts(p), Conjuncts(q))
		-> Conjuncts(<union>(p, q))
								
conjoin:
	And(Conjuncts(p), q)
		-> Conjuncts(<union>(p, [q]))						
		
conjoin:
	And(q, Conjuncts(p))
		-> Conjuncts(<union>(p, [q]))								
		