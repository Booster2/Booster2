	simplify:
		Choice(Assign(p,e), q)
			-> Assign(p,e)
			
/*	simplify:
		Choice(Guard(g1, Parallel(Parallel(p1, p2), p3)),
			   Guard(Not(g2), p1))
		-> Parallel(p1, Choice(Guard(g1, Parallel(p2,p3)), Guard(Not(g1), Skip())))		
		
	simplify:
		Choice(Parallel(b,a),
			   Parallel(c,a))
		->
			Parallel(a, Choice(b,c))
