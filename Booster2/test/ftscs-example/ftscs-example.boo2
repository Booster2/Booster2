system fstcsExample

class A {
	attributes
		m : Int
		b : [B.a]
	invariants
		m < 10
	methods
		increment { m' = m + 1} 
}

class B {
	attributes
		a : [A.b]
		n : Int
		cp : set(C.bp)[*]
		cq : set(C.bq)[*]

	invariants
		a.m < n 
	methods
		increment {n' = n + 1}

/*** Method temporarily removed because it's not parsing! */
/***	addQ { c? : cq'}*/

}

class C {
	attributes
		o : Int
		bp : B.cp
		bq : [B.cq]
	invariants
		bp /= bq
	
}
