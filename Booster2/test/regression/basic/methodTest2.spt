/**
* Unit tests for the Booster2 language.
*/
module template


language Booster2




test Method test 2 [[
  
system methodTest

   class class1 {
   	  attributes
   	  	att1 : INT
   	  methods
  	  	m1 { att1' = 5 + 3 }	
	}

  
]] run parse to 

System(
  "methodTest"
, [ Class(
      "class1"
    , [ Attributes([Attribute("att1", BasicType(Int()))])
      , Methods(
          [ Method(
              "m1"
            , BinRel(
                PathStart("att1", Some(Primed()))
              , Equal()
              , BinOp(BasicValue(Integer("5")), Plus(), BasicValue(Integer("3")))
              )
            )
          ]
        )
      ]
    )
  ]
)
