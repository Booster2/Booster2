/**
* Unit tests for the Booster2 language.
*/
module template


language Booster2




test Method test 1 [[
  
system methodTest

   class class1 {
   	  attributes
   	  	att1 : INT
   	  methods
  	  	m1 { att1' = 5 }	
	}
  
  
]] run expand-booster-for-testing to 

System(
  "methodTest"
, [ Class(
      "class1"
    , [ Attributes([Attribute("att1", BasicType(Int()))])
      , Methods(
          [ Method(
              "m1"
            , BinRel(
                PathStart("att1", Some(Primed()))
              , Equal()
              , BasicValue(Integer("5"))
              )
            )
          ]
        )
      ]
    )
  ]
)