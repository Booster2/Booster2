system ClassicCars 


class ProductLine {
	attributes
		productLine : STRING
		textDescription : STRING
		htmlDescription : STRING
		image : STRING
		products : set ( Product . productLine  ) [ 0 .. * ]
		
	invariants
		true
	methods
		updateText { t? : STRING 
		               --> ( this.textDescription := t? ) }
		
}
class Product {
	attributes
		productName : STRING
		productCode : STRING
		productLine : ProductLine . products
		productScale : STRING
		productVendor : STRING
		productDescription : STRING
		quantityInStock : INT
		buyPrice : STRING
		MSRP : STRING
		forPurchaseOrders : set ( PurchaseOrderDetails . product  ) [ 0 .. * ]
		
	invariants
		true
	methods
		
}
class PurchaseOrderDetails {
	attributes
		purchaseOrder : PurchaseOrder . details
		product : Product . forPurchaseOrders
		quantityOrdered : INT
		priceEach : STRING
		orderLineNumber : INT
		
	invariants
		true
	methods
		
}
class PurchaseOrder {
	attributes
		orderNumber : INT
		orderDate : STRING
		requiredDate : STRING
		shippedDate : STRING
		status : STRING
		comments : STRING
		details : set ( PurchaseOrderDetails . purchaseOrder  ) [ 0 .. * ]
		customer : Customer . orders
		
	invariants
		true
	methods
		
}
class Customer {
	attributes
		customerNumber : INT
		customerName : STRING
		contactLastName : STRING
		contactFirstName : STRING
		phone : STRING
		addressLine1 : STRING
		addressLine2 : STRING
		city : STRING
		state : STRING
		postalCode : STRING
		country : STRING
		salesRepEmployee : Employee . salesRepFor
		creditLimit : STRING
		orders : set ( PurchaseOrder . customer  ) [ 0 .. * ]
		payments : set ( Payment . customer  ) [ 0 .. * ]
		
	invariants
		true
	methods
		
}
class Payment {
	attributes
		customer : Customer . payments
		checkNumber : STRING
		paymentDate : STRING
		amount : STRING
		
	invariants
		true
	methods
		
}
class Employee {
	attributes
		employeeNumber : INT
		lastName : STRING
		firstName : STRING
		extension : STRING
		email : STRING
		office : Office . employees
		reportsTo : [ Employee . inChargeOf ]
		jobTitle : STRING
		inChargeOf : set ( Employee . reportsTo  ) [ 0 .. * ]
		salesRepFor : set ( Customer . salesRepEmployee  ) [ 0 .. * ]
		
	invariants
		true
	methods
		
}
class Office {
	attributes
		officeCode : INT
		city : STRING
		phone : STRING
		addressLine1 : STRING
		addressLine2 : STRING
		state : STRING
		country : STRING
		postalCode : STRING
		territory : STRING
		employees : set ( Employee . office  ) [ 0 .. * ]
		
	invariants
		true
	methods
		
}

