system workflowTest

class C1 {
	attributes
   	  	x : INT
   	  methods
  	  	add1 { x' = x + 1 }
  	  	
  	  	sub1 { x' = x - 1 }
  	  invariants
  	    x >= 0 & x <= 2 
  	  workflows
  	    seq w3: SKIP 
  	    seq w1: Normal true & add1() -> w2
  	    seq w2: Normal true & sub1() -> w3
  	    par p: w1 ||| w2
}
