system workflowTest

class C1 {
	attributes
   	  	x : INT
   	  methods
  	  	add1 { x' = x + 1 }
  	  	
  	  	sub1 { x' = x - 1 }
  	  invariants
  	    x >= 0 & x <= 2 
  	  workflows
  	    seq w0: Normal true & add1() -> Wait.0.1 -> Wait.1.0 -> Normal true & add1() -> SKIP
  	    seq w1: Wait.0.1 -> Normal true & sub1() -> Wait.1.0 -> SKIP
  	    //seq w0: Wait.0.1 -> SKIP
  	    //seq w1: Wait.0.1 -> SKIP
  	    par p: w0 ||| w1
}
