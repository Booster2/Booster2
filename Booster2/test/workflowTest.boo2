system workflowTest

class C1 {
	attributes
   	  	x : INT
   	  methods
  	  	add1 { x' = x + 1 }
  	  	
  	  	sub1 { x' = x - 1 }
  	  invariants
  	    x >= 2 & x <= 4 
  	  workflows
  	    seq a1: true & m1() -> true & m1() -> a2
  	            []
  	            true & m1() -> a2
  	    seq a2: true & m2() -> a4
  	    seq a3: true & m3() -> a2
  	    seq a4: true & m4() -> SKIP
  	    
  	  workflows
  	    par p1: a2 ||| a2
}


class C2 {
	attributes
   	  	x : INT
   	  methods
  	  	add1 { x' = x + 1 }
  	  	
  	  	sub1 { x' = x - 1 }
  	  invariants
  	    x >= 2 & x <= 4 
  	  workflows
  	    seq w0: true & add1() -> Wait.0.1 ->                  Wait.1.0 -> true & add1() -> SKIP
  	    seq w1:                  Wait.0.1 -> true & sub1() -> Wait.1.0 -> SKIP
  	  workflows
  	    par p: w0 ||| w1
}

