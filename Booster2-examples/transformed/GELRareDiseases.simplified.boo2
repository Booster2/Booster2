system RareDiseasesSimplified
 


	class GELDiseaseGroup  { 		attributes			name (ID) : STRING			identifier  : STRING			subGroups  : set ( GELDiseaseSubGroup. parentGroups )  [0 .. *]		methods 			create { (identifier? : STRING & name? : STRING) --> ( ((gdg!.identifier := identifier?) || (gdg!.name := name?)) || (new gdg! : GELDiseaseGroup) ) }			newSubGroup { (((identifier? : STRING & name? : STRING) & this : GELDiseaseGroup) & this.subGroups <<: GELDiseaseSubGroup \/ {gdsg!}) --> ( ((((gdsg!.identifier := identifier?) || (gdsg!.name := name?)) || (gdsg!.parentGroups := gdsg!.parentGroups \/ {this})) || (new gdsg! : GELDiseaseSubGroup)) || (this.subGroups := this.subGroups \/ {gdsg!}) ) }	}	class GELDiseaseSubGroup  { 		attributes			name (ID) : STRING			identifier  : STRING			parentGroups  : set ( GELDiseaseGroup. subGroups )  [0 .. *]			specificDisorders  : set ( GELSpecificDisorder. subGroups )  [0 .. *]		methods 			create { (identifier? : STRING & name? : STRING) --> ( ((gdsg!.identifier := identifier?) || (gdsg!.name := name?)) || (new gdsg! : GELDiseaseSubGroup) ) }			newSpecificDisorder { (((identifier? : STRING & name? : STRING) & this : GELDiseaseSubGroup) & this.specificDisorders <<: GELSpecificDisorder \/ {gsd!}) --> ( ((((gsd!.identifier := identifier?) || (gsd!.name := name?)) || (gsd!.subGroups := gsd!.subGroups \/ {this})) || (new gsd! : GELSpecificDisorder)) || (this.specificDisorders := this.specificDisorders \/ {gsd!}) ) }	}	class GELSpecificDisorder  { 		attributes			name (ID) : STRING			identifier  : STRING			subGroups  : set ( GELDiseaseSubGroup. specificDisorders )  [0 .. *]		methods 			create { (identifier? : STRING & name? : STRING) --> ( ((gsd!.identifier := identifier?) || (gsd!.name := name?)) || (new gsd! : GELSpecificDisorder) ) }	}	class HPOPhenotype  { 		attributes			name (ID) : STRING			identifier (ID) : STRING			description  : STRING			broaderTerms  : set ( HPOPhenotype. narrowerTerms )  [0 .. *]			narrowerTerms  : set ( HPOPhenotype. broaderTerms )  [0 .. *]		methods 			create { ((description? : STRING & identifier? : STRING) & name? : STRING) --> ( (((new phen! : HPOPhenotype) || (phen!.description := description?)) || (phen!.identifier := identifier?)) || (phen!.name := name?) ) }			associateNarrowerTerm { (narrowerTerm? : HPOPhenotype & this : HPOPhenotype) --> ( (narrowerTerm?.broaderTerms := narrowerTerm?.broaderTerms \/ {this}) || (this.narrowerTerms := this.narrowerTerms \/ {narrowerTerm?}) ) }			associateBroaderTerm { (broaderTerm? : HPOPhenotype & this : HPOPhenotype) --> ( (broaderTerm?.narrowerTerms := broaderTerm?.narrowerTerms \/ {this}) || (this.broaderTerms := this.broaderTerms \/ {broaderTerm?}) ) }	}	class Site  { 		attributes			name (ID) : STRING			identifier  : STRING	}